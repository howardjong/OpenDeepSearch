modules = ["python-3.10"]
run = "python gradio_demo.py"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "python gradio_demo.py"]

[workflows]
runButton = "Run with DeepSeek"

[[workflows.workflow]]
name = "Run Demo"
author = 35006224
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python gradio_demo.py --reranker jina"

[[workflows.workflow]]
name = "Run Simple Demo"
author = 35006224
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python gradio_demo.py --reranker jina"

[[workflows.workflow]]
name = "Run with Gemini Pro"
author = 35006224
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python gradio_demo.py --reranker jina --model-name \"openrouter/google/gemini-2.5-pro-exp-03-25\" --orchestrator-model \"openrouter/google/gemini-2.5-pro-exp-03-25\""

[[workflows.workflow]]
name = "Run with Gemini 2.5 Pro"
author = 35006224
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python gradio_demo.py --reranker jina --model-name \"openrouter/google/gemini-2.5-pro-preview-03-25\" --orchestrator-model \"openrouter/google/gemini-2.5-pro-preview-03-25\""

[[workflows.workflow]]
name = "Run with DeepSeek"
author = 35006224
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python gradio_demo.py --reranker jina --model-name \"openrouter/deepseek/deepseek-r1-zero:free\" --orchestrator-model \"openrouter/deepseek/deepseek-r1-zero:free\""

[[ports]]
localPort = 7860
externalPort = 80
